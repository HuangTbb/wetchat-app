/**index.wxss**/
.main {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.circle {
  width: 50px;
  height: 50px;
  display: inline-block;
  text-align: center;
  background: #f8f8f8;
  border-radius: 50%;
  line-height: 50px;
  margin: 30px 0;
}
.number {
  display: flex;
  margin: 30px 0;
}
.numberX,
.numberY {
  padding: 12px 30px;
  background: #f8f8f8;
  margin: 0 10px;
  border-radius: 5px;
}
.comfirm {
  margin: 30px 0;
}
